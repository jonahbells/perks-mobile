{
  "expo": {
    "name": "Perks Mania",
    "slug": "Perksmania-Mobile",
    "scheme": "perskmania",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/perkmania_launcher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mherbm.PerksmaniaMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mherbm.PerksmaniaMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.40387580751-prk76d7fdf6gr8mljsnhsoa2q939suc8"
        }
      ]
    ],
    "owner": "jonah.delrosario",
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "74563559-7f65-40b0-91b3-9e534e427ac0"
      }
    }
  }
}